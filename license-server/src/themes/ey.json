{
  "$schema": "https://opencode.ai/theme.json",
  "name": "EY",
  "description": "Official EY color scheme - yellow corporate branding",
  "author": "Snow-Flow Team",
  "version": "1.0.0",
  "defs": {
    "ey_yellow": "#FFE600",
    "ey_yellow_light": "#FFED4E",
    "ey_yellow_dark": "#D4C000",
    "ey_dark_gray": "#2E2E38",
    "ey_purple": "#6E3FA3",
    "ey_teal": "#00B5C8",
    "ey_green": "#5BB85C",
    "ey_orange": "#FF8C2B",
    "ey_red": "#E53935",
    "ey_gray_darker": "#1A1A1A",
    "ey_gray_dark": "#2B2F33",
    "ey_gray_medium": "#3E4345",
    "ey_gray": "#5F6D7E",
    "ey_gray_light": "#8B95A5",
    "ey_gray_lighter": "#D1D8DD",
    "ey_white": "#FFFFFF",
    "ey_off_white": "#F4F6F8",
    "ey_code_bg": "#282C34",
    "ey_selection": "#FFE600",
    "ey_selection_dark": "#665C00"
  },
  "theme": {
    "primary": "ey_yellow",
    "secondary": "ey_purple",
    "accent": "ey_teal",
    "error": "ey_red",
    "warning": "ey_orange",
    "success": "ey_green",
    "info": "ey_teal",
    "text": {
      "dark": "ey_white",
      "light": "ey_dark_gray"
    },
    "textMuted": {
      "dark": "ey_gray_light",
      "light": "ey_gray"
    },
    "background": {
      "dark": "ey_gray_dark",
      "light": "ey_white"
    },
    "backgroundAlt": {
      "dark": "ey_gray_medium",
      "light": "ey_off_white"
    },
    "border": {
      "dark": "ey_gray_medium",
      "light": "ey_gray_lighter"
    },
    "borderActive": "ey_yellow",
    "selection": {
      "dark": "ey_selection_dark",
      "light": "ey_selection"
    },
    "cursor": "ey_yellow",
    "diffAdded": "ey_green",
    "diffRemoved": "ey_red",
    "diffModified": "ey_orange",
    "syntaxComment": {
      "dark": "ey_gray_light",
      "light": "ey_gray"
    },
    "syntaxKeyword": "ey_purple",
    "syntaxString": "ey_green",
    "syntaxNumber": "ey_orange",
    "syntaxFunction": "ey_teal",
    "syntaxVariable": {
      "dark": "ey_white",
      "light": "ey_dark_gray"
    },
    "syntaxType": "ey_teal",
    "syntaxConstant": "ey_orange",
    "syntaxOperator": "ey_purple",
    "syntaxPunctuation": {
      "dark": "ey_gray_light",
      "light": "ey_gray"
    },
    "syntaxAttribute": "ey_teal",
    "syntaxTag": "ey_red",
    "syntaxError": "ey_red",
    "linkText": "ey_purple",
    "linkUnderline": "ey_purple",
    "codeBackground": {
      "dark": "ey_code_bg",
      "light": "ey_off_white"
    },
    "codeBlockBackground": {
      "dark": "ey_gray_darker",
      "light": "ey_off_white"
    },
    "promptBackground": {
      "dark": "ey_gray_medium",
      "light": "ey_off_white"
    },
    "toolCallBackground": {
      "dark": "ey_gray_medium",
      "light": "ey_off_white"
    },
    "headerBackground": {
      "dark": "ey_dark_gray",
      "light": "ey_yellow"
    },
    "headerText": {
      "dark": "ey_white",
      "light": "ey_dark_gray"
    },
    "spinnerPrimary": "ey_yellow",
    "spinnerSecondary": "ey_purple",
    "scrollbarThumb": {
      "dark": "ey_gray",
      "light": "ey_gray_light"
    },
    "scrollbarTrack": {
      "dark": "ey_gray_dark",
      "light": "ey_gray_lighter"
    }
  }
}
